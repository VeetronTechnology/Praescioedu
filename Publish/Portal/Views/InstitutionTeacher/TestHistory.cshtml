@section Scripts{

    <script type="text/javascript">
        myApp.controller('TestHistoryCtrl', function ($scope, $rootScope, $filter, NetworkService, $timeout) {
            var formdata = new FormData();

            $scope.TitleName = 'Test History'

            NetworkService.get('PraescioCommon/GetQuestionByAssignmentId?assignmentid=@ViewBag.Assignmentid').success(function (data) {
                $scope.Assignment = data.Assignment;
                $scope.QuestionList = data.Question;
                console.log(123);

                $('#AssignmentPDF').append('<embed id="pdfFile" type="application/pdf" src="/' + data.Assignment.UploadFileSrc + '" height="500px" width="80%" internalinstanceid="37" title=""><br><br><br>')
            });

            $scope.isQuestionContain = function (questionType) {
                var result = false;

                $.each($scope.QuestionList, function (i, e) {
                    if ($scope.QuestionList[i].QuestionTypeId == questionType) {
                        result = true;
                    }
                })

                return result;
            }

            $scope.IsSubmitted = function ($event) {
                var prevent = false;
                $.each($scope.QuestionList, function (i, e) {
                    if ($scope.QuestionList[i].IsUserSubmitted == true) {
                        console.log($scope.QuestionList[i].QuestionType.Name);
                        console.log($scope.QuestionList[i].IsUserSubmitted);
                        prevent = true;
                    }
                })
                if (prevent)
                    $event.preventDefault();
                $event.preventDefault();
            }


        });
    </script>
}


